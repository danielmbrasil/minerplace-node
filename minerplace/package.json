{
  "name": "minerplace",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "scripts": {
    "dev:js": "nodemon",
    "dev": "concurrently npm:dev:*",
    "db:create:dev": "createdbjs ${DATABASE_DB:-minerplace_dev} --user=${DATABASE_USER:-postgres} --password=${DATABASE_PASSWORD:-postgres} --host=${DATABASE_HOST:-127.0.0.1}",
    "db": "node --loader ts-node/esm ./node_modules/.bin/knex",
    "db:migrate:dev": "npm run db migrate:latest",
    "db:rollback:dev": "npm run db migrate:rollback",
    "db:seed:dev": "npm run db seed:run",
    "db:create:test": "createdbjs ${TEST_DATABASE_DB:-minerplace_test} --user=${DATABASE_USER:-postgres} --password=${DATABASE_PASSWORD:-postgres} --host=${DATABASE_HOST:-127.0.0.1}",
    "db:migrate:test": "NODE_ENV=test npm run db migrate:latest --env test",
    "db:rollback:test": "NODE_ENV=test npm run db migrate:rollback --env test",
    "test": "NODE_ENV=test jest --runInBand"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tsconfig/node-lts": "^18.12.5",
    "@types/ejs": "^3.1.5",
    "@types/knex-cleaner": "^1.3.6",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "env-cmd": "^10.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/nodemailer": "^6.4.14",
    "ajv-formats": "^2.1.1",
    "concurrently": "^8.2.2",
    "connect-redis": "^7.1.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "ioredis": "^5.3.2",
    "knex": "^2.5.1",
    "nodemailer": "^6.9.8",
    "objection": "^3.1.3",
    "pg": "^8.11.3",
    "pgtools": "^1.0.1"
  }
}
